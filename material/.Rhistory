par( mfrow = c( 1, 1 ) )
plot(l5_2011$red, l5_2011$nir)
abline(soil$BSL, col="red")
library(rgdal)
vector <- readOGR(dsn="vector_data/", layer="extract")
vector
datos <- extract(l5_2011,vector)
plot(l5_2011$red, l5_2011$nir)
points(as.data.frame(datos[1])$red,as.data.frame(datos[1])$nir, col="green")
points(as.data.frame(datos[2])$red,as.data.frame(datos[2])$nir, col="red")
abline(soil$BSL, col="red")
plot(l5_2011$red, l5_2011$green)
points(as.data.frame(datos[1])$red,as.data.frame(datos[1])$green, col="green")
points(as.data.frame(datos[2])$red,as.data.frame(datos[2])$green, col="red")
plot(l5_2011$red, l5_2011$swir1)
plot(l5_2011$nir, l5_2011$swir1)
points(as.data.frame(datos[2])$nir,as.data.frame(datos[2])$swir1, col="red")
points(as.data.frame(datos[1])$nir,as.data.frame(datos[1])$swir1, col="green")
