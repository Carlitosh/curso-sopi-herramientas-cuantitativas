library("landsat")
BSL(band3 = l5_2011$red, band4 = l5_2011$nir)
BSL(l5_2011$red, l5_2011$nir)
BSL(as.data.frame(l5_2011$red), as.data.frame(l5_2011$nir))
soil <- BSL(as.data.frame(l5_2011$red), as.data.frame(l5_2011$nir))
soil
